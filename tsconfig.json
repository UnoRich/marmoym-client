{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": ".",
    "jsx": "react",
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "outDir": "tsc_out",
    "paths": {
      "@actions/*": [
        "src/state/actions/*"
      ],
      "@apis/*": [
        "src/apis/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@constants/*": [
        "src/constants/*"
      ],
      "@containers/*": [
        "src/containers/*"
      ],
      "@models/*": [
        "src/models/*"
      ],
      "@selectors/*": [
        "src/state/selectors/*"
      ],
      "@src/*": [
        "src/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
    },
    "target": "es6"
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
  ]
}